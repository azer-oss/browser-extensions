{
  "manifest_version": 2,
  "name": "Kozmos",
  "description": "The simpliest way to bookmark pages, and find them when you need.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/heart-icon.png",
    "default_title": "Kozmos"
  },
  "background": {
    "scripts": ["build/background.js"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "http://*/*"
  ]
}
